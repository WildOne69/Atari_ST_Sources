 1:* PROCEDURE V_GET
 2:* GET-FILE ZUR šBERPRšFUNG DER BENUTZEREINGABEN
 3:
 4:* VARIABLEN DEFINITION
 5: Y.MSG = ''
 6: Y.TMP = GETNAME()
 7:* šBERPRšFUNG
 8: DO CASE
 9: CASE .NOT. NRANGE(VAL($(&Y.TMP,1,2)),0,23) .AND. GETNO() = 2
10:         Y.MSG = 'EINGABE-BEREICH STUNDEN -> 00-23'
11: CASE .NOT. NRANGE(VAL($(&Y.TMP,1,2)),0,24) .AND. GETNO() = 3
12:         Y.MSG = 'EINGABE-BEREICH STUNDEN -> 00-24'
13: CASE .NOT. NRANGE(VAL($(&Y.TMP,4,2)),0,59)
14:         Y.MSG = 'EINGABE-MINUTEN -> 00-59'
15: CASE (AT('-',TIMESUB(ABRV->BIS,ABRV->VON)) <> 0 ;
16:         .OR. TIMETON(TIMESUB(ABRV->BIS,ABRV->VON),2) > 24 ;
17:         .OR. ABRV->VON = ABRV->BIS) .AND. GETNO()= 3
18:         Y.MSG = 'ANFANG GR™SSER ENDE !!'
19:*        *GET-VARIABLEN NEU BELEGEN
20:         Y.TMP = VON
21:         P.VON = P.BIS
22:         P.BIS = Y.TMP
23:         ASSIGN USERSTAT(-1)
24: ENDCASE
25: * END KONTROLLE UND EINSTELLUNG
26: IF  .NOT. '!!'$Y.MSG .AND. Y.MSG <> ''
27:         STORE '00:00' TO &Y.TMP
28:         ASSIGN USERSTAT(-1)
29: ENDIF
30: REFRESH GETS
31: * AUSGABE FEHLERMEDLUNG
32: @ 12,9 ESAY Y.MSG
33: RETURN
