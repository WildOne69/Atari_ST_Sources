FUNCTION speicher_kill(modus|)
  LOCAL back%
  SELECT modus|
  CASE 0
    ' Speicherverwaltung abmelden
    back%=MFREE(malloc_adr%)
  CASE 1
    ' ges. Freispeicher
    back%={malloc_adr%+8}
  CASE 2
    ' gr”žter Speicherblock
    back%={{malloc_adr%+24}}
  ENDSELECT
  RETURN back%
ENDFUNC
