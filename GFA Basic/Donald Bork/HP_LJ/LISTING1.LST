#include <stdio.h>
#define druck(a)    fprintf(stdprn, a)

void main(void)
{

druck("\033%%-12345X@PJL \n");     /* PJL einleiten */

druck("@PJL DEFAULT DENSITY = 3\n");
druck("@PJL DEFAULT MANUALFEED = OFF\n");
druck("@PJL DEFAULT LPARM : PCL SYMSET = PC8\n");
druck("@PJL DEFAULT LPARM : PCL FONTNUMBER = 15\n");
druck("@PJL DEFAULT LPARM : PCL PTSIZE = 12.00\n");

druck("\033%%-12345X");            /* und aus PJL verabschieden */
}

