'
> PROCEDURE fade(p1%,p2%)
  FOR c=0 TO 30 STEP 2
    cl=CARD{p1%+c}
    cr=(cl AND 7)/7
    cg=(cl AND 112)/112
    cb=(cl AND 1792)/1792
    '
    FOR fade=0 TO 6
      cl2=INT(cr*fade)+INT(cg*fade)*16+INT(cb*fade)*256
      CARD{p2%+c+(7-fade)*32}=cl2
    NEXT fade
  NEXT c
  BMOVE p1%,p2%,32
RETURN
'
> PROCEDURE fade2(col%,p2%)
  cl=col%
  cr=(cl AND 7)/7
  cg=(cl AND 112)/112
  cb=(cl AND 1792)/1792
  '
  FOR fade=0 TO 6
    cl2=INT(cr*fade)+INT(cg*fade)*16+INT(cb*fade)*256
    CARD{p2%+fade*2}=cl2
  NEXT fade
  CARD{p2%+14}=col%
RETURN
