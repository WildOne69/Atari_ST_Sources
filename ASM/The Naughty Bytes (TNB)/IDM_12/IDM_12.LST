> PROCEDURE init
  DIM reg%(16)
  reg%(0)=1
  '
  BMOVE &HFF8240,oldpal%,32
  '
  res%=XBIOS(4)
  '
  VOID XBIOS(5,L:-1,L:-1,0)
  '
  LONG{pal0%}=0
  LONG{pal0%+4}=0
  LONG{pal0%+8}=0
  LONG{pal0%+12}=0
  LONG{pal0%+16}=0
  LONG{pal0%+20}=0
  LONG{pal0%+24}=0
  LONG{pal0%+28}=0
  '
  change_pal(pal0%)
  '
  LONG{pal0%}=0
  '
  FOR z=0 TO 35
    BMOVE dist_logos%+z*40,oldscr%+z*160,40
  NEXT z
  '
  GET 0,0,80,35,log$
  '
  CLS
  FOR s=15 DOWNTO 0
    PUT s,0,log$
    FOR z=0 TO 35
      BMOVE oldscr%+z*160,moondist%+s*1440+z*40,40
    NEXT z
  NEXT s
  '
  FOR z=0 TO 35
    BMOVE dist_logos%+1440+z*40,oldscr%+z*160,40
  NEXT z
  '
  GET 0,0,80,35,log$
  '
  CLS
  FOR s=15 DOWNTO 0
    PUT s,0,log$
    FOR z=0 TO 35
      BMOVE oldscr%+z*160,drizztdist%+s*1440+z*40,40
    NEXT z
  NEXT s
  '
  FOR z=0 TO 136
    BMOVE kos%+z*64,oldscr%+(z+32)*160+48,64
  NEXT z
  '
  GOSUB fade(kospal%,fade%)
  '
  BMOVE logo_pal%,pal3%,32
  CARD{pal3%}=&H2
  GOSUB fade(pal3%,fade3%)
  '
  cnt%=0
  FOR p=0 TO 11
    FOR z=0 TO 17
      READ a$
      FOR s=0 TO 39
        a=PEEK(VARPTR(a$)+s)
        IF a>=97 AND a<=123 THEN
          SUB a,97
        ELSE IF a>=48 AND a<=57
          SUB a,22
        ELSE IF a=63
          a=36
        ELSE IF a=33
          a=37
        ELSE IF a=46
          a=38
        ELSE IF a=44
          a=39
        ELSE
          a=40
        ENDIF
        IF a<>40 THEN
          IF (s AND 1)=1 THEN
            p%=1120+a*28
          ELSE
            p%=a*28
          ENDIF
        ELSE
          p%=2240
        ENDIF
        CARD{text%+cnt%}=p%
        ADD cnt%,2
      NEXT s
    NEXT z
  NEXT p
  '
  FOR pal=6 DOWNTO 0
    VSYNC
    VSYNC
    GOSUB change_pal(fade%+pal*32)
  NEXT pal
  '
  GOSUB fade(drizztpal%,fade4%)
  '
  RCALL music2%,reg%()
  LONG{&H4D2}=music2%+8
  '
  scr%=sc% AND &HFFFF00
  scr2%=sc2% AND &HFFFF00
  '
  FOR s%=scr% TO scr%+39996 STEP 4
    LONG{s%}=0
  NEXT s%
  BMOVE scr%,scr2%,40000
  '
  BMOVE rastertitdef%,rastertit%,396
  '
  FOR deg%=0 TO 358 STEP 2
    z=INT(SINQ(deg%)*65+17)*2
    LONG{sinus%+deg%*2}=rastertit%+z
  NEXT deg%
  '
  cnt%=0                        ! calculate stars
  FOR col=0 TO 2
    FOR st=0 TO 29
      x=RANDOM(320)
      y=RANDOM(180)
      py=y*160+8640
      FOR s=0 TO 321
        SUB x,(col+1)
        IF x<0 THEN
          ADD x,320
        ENDIF
        px=INT(x/16)
        px2=(x-px*16)*4+col*64
        MUL px,8
        CARD{stars%+cnt%}=px+py+4
        CARD{stars%+cnt%+2}=px2
        ADD cnt%,4
      NEXT s
    NEXT st
  NEXT col
  '
  reg%(0)=8
  RCALL music2%,reg%()
  LONG{&H4D2}=music2%+8
  '
  FOR pal=1 TO 7
    VSYNC
    VSYNC
    GOSUB change_pal(fade%+pal*32)
  NEXT pal
  '
  CLS
  '
  VOID XBIOS(5,L:scr%,L:scr%,0)
  '
  FOR vs=0 TO 24
    VSYNC
  NEXT vs
  '
  GOSUB musix_off
  '
  GOSUB musix_on
RETURN
