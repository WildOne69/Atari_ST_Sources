Open "O",#1,"SLEEP.PRG"
Cls
Fertig$="Ende"
Do
  Read Dat$
  Inc Cnt%
  Exit If Instr(Dat$,Fertig$,1)>0
  Dat$=Right$(Dat$,Len(Dat$)-1)
  If (Cnt% Mod 12)=1
    Lin%=Val(Dat$)
    Chk%=0
    Print At(38,25);Lin%;
  Else
    Dat$="&"+Dat$
    If (Cnt% Mod 12)=0
      If Chk%<>Val(Dat$)
        Print " => Fehler!"
        Chk%=0
      Endif
    Else
      Add Chk%,Val(Dat$)
      Out #1,Val(Dat$)
    Endif
  Endif
Loop
Read Dat$
If Chk%<>Val("&"+Right$(Dat$,Len(Dat$)-1))
  Print " => Fehler!"
Endif
