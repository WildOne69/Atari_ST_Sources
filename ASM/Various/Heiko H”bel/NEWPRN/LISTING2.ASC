PRINT "AUSGABETEST: 32000 BYTE AN CENTRONICS" 
PRINT 
PRINT "Ohne NEWPRN.PRG:" 
teste 
VOID EXEC(0,"NEWPRN.PRG","","") 
PRINT 
PRINT "Mit NEWPRN.PRG:" 
teste 
PRINT 
PRINT "Bitte Maustaste drÅcken" 
REPEAT 
UNTIL MOUSEK 
PROCEDURE teste 
  t=TIMER 
  FOR i%=1 TO 32000 
    VOID GEMDOS(5,64) 
  NEXT i% 
  x=TIMER-t 
  PRINT "Zeit fÅr  GEMDOS 5-Ausgabe: ";x/200;" s" 
  t=TIMER 
  FOR i%=1 TO 32000 
    VOID BIOS(3,0,64) 
  NEXT i% 
  x=TIMER-t 
  PRINT "Zeit fÅr    BIOS 3-Ausgabe: ";x/200;" s" 
  t=TIMER 
  LPRINT STRING$(32000,CHR$(64)) 
  x=TIMER-t 
  PRINT "Zeit fÅr GEMDOS 64-Ausgabe: ";x/200;" s" 
RETURN
